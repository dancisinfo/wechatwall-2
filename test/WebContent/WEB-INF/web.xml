<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>wallServlet</servlet-name>
		<servlet-class>com.wedding.servlet.WallServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>invitationServlet</servlet-name>
		<servlet-class>com.wedding.servlet.InvitationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>wallServlet</servlet-name>
		<url-pattern>/wallServlet.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>invitationServlet</servlet-name>
		<url-pattern>/invitationServlet.do</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>